<?xml version="1.0" encoding="utf-8"?>
<phpcompatinfo reference="ALL"
               report="summary"
               reportFileAppend="false"
               cacheDriver="file"
               recursive="true"
               fileExtensions="php, htm"
               consoleProgress="false"
               verbose="false">

    <excludes>
        <exclude id="video">
            <directory name="config"/>
            <directory name="css"/>
            <directory name="doc"/>
            <directory name="help"/>
            <directory name="images"/>
            <directory name="languages"/>
            <directory name="lib"/>
            <directory name="tests"/>
        </exclude>
    </excludes>
</phpcompatinfo>
